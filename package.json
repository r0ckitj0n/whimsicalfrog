    "guard:docs:filenames:staged": "node scripts/dev/guard-docs-filenames.mjs --staged",
    "guard:images": "node scripts/guard-images.mjs",
    "guard:templates:css": "node scripts/guard-templates-css.mjs",
    "guard:icons": "node scripts/guards/guard-icons-consistency.mjs",
    "guard:modal-utilities": "node scripts/guards/guard-modal-panel-utilities.mjs",
    "guard:email-config": "node scripts/guards/guard-email-config.mjs",
    "guard:repo-hygiene": "node scripts/guards/guard-repo-hygiene.mjs",
    "guard:z-index": "node scripts/guard-z-index.mjs",
    "guard:no-raw-fetch": "node scripts/guards/no-raw-fetch.mjs",
