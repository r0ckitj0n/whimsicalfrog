    "tooltips:audit": "node scripts/dev/audit-tooltips.mjs",
    "tooltips:seed:dry": "node scripts/dev/seed-admin-tooltips.mjs --dry-run",
    "tooltips:seed": "node scripts/dev/seed-admin-tooltips.mjs --apply",
    "tooltips:curate:dry": "DRY_RUN=1 node scripts/dev/curate_unique_tooltips.mjs",
    "tooltips:curate": "node scripts/dev/curate_unique_tooltips.mjs",
    "tooltips:restore": "node scripts/dev/restore-tooltips-from-backup.mjs",
    "tooltips:prune:dry": "node scripts/dev/prune-stale-tooltips-by-codegrep.mjs --dry-run",
    "tooltips:prune": "node scripts/dev/prune-stale-tooltips-by-codegrep.mjs --apply",
    "smoke:apis": "bash scripts/guards/smoke_all_apis.sh",
    "smoke:visual": "node scripts/dev/visual-smoke.mjs",
    "smoke:overlays": "node scripts/dev/frontend-tests/overlay-smoke.mjs",
    "smoke:cart": "node scripts/dev/frontend-tests/cart-smoke.mjs",
    "smoke:admin:crawl": "node scripts/dev/frontend-tests/admin-crawl-smoke.mjs",
    "smoke:store:crawl": "node scripts/dev/frontend-tests/store-crawl-smoke.mjs",
    "test:overlays": "node scripts/dev/frontend-tests/overlay-suite.mjs",
    "test:attributes-inline": "node scripts/dev/frontend-tests/attributes-inline.mjs",
    "db:dump": "bash scripts/db/dump_local_db.sh",
    "db:dump:gzip": "bash scripts/db/dump_local_db.sh --gzip",
    "release": "bash scripts/release.sh",
    "release:no-deploy": "bash scripts/release.sh --no-deploy",
    "release:tag": "bash scripts/release.sh --no-deploy --no-build --no-push",
    "precommit": "echo '[precommit] disabled (see .cursorrules for rules)'",
    "pm2:start": "npx pm2 start pm2.config.cjs",
    "pm2:restart": "npx pm2 restart wf-vite",
    "pm2:stop": "npx pm2 stop wf-vite || true",
    "pm2:status": "npx pm2 status wf-vite",
    "pm2:logs": "npx pm2 logs wf-vite",
    "pm2:save": "npx pm2 save",
    "pm2:startup": "npx pm2 startup"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.25.0",
