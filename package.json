    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "baseline-browser-mapping": "^2.9.11",
    "eslint": "^9.11.1",
    "glob": "^11.0.3",
    "lint-staged": "^15.5.2",
    "pm2": "^5.4.2",
    "postcss": "^8.5.6",
