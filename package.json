    "stop": "rm hot",
    "checkpoint": "node scripts/dev/checkpoint.mjs",
    "quarantine:dupes": "bash scripts/maintenance/quarantine_duplicates.sh",
    "guards": "echo '[guards] disabled (see .cursorrules for rules)'",
    "prebuild": "echo '[prebuild] disabled (see .cursorrules for rules)'",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "echo '[lint] disabled (see .cursorrules for rules)'",
    "lint:fix": "echo '[lint:fix] disabled (see .cursorrules for rules)'",
    "lint:css": "echo '[lint:css] disabled (see .cursorrules for rules)'",
    "lint:css:fix": "echo '[lint:css:fix] disabled (see .cursorrules for rules)'",
    "lint:all": "echo '[lint:all] disabled (see .cursorrules for rules)'",
